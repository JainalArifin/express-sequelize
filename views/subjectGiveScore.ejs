<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Give Score</h1>
    <form action="/subject/<%= dtStudent.id %>/<%= dtSubject.id %>/givescore" method="post">
    <input type="text" name="StudentId" value="<%= dtStudent.id %>">
    <input type="text" name="first_name" value="<%= dtStudent.first_name %>">
    <input type="text" name="last_name" value="<%= dtStudent.last_name %>">
    <input type="text" name="subject_name" value="<%= dtSubject.subject_name %>">
    <input type="text" name="score" placeholder="Input Score">
    <button type="submit" name="submit">Save</button>
    </form>
  </body>
</html>
