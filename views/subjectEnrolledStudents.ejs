<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1><%= dtSubjectStudent[0].Subject.subject_name %></h1>
    <table>
      <tr>
        <th>No</th>
        <th>Student Name</th>
        <th>Score</th>
      </tr>
      <% var no = 0;  for(var i = 0; i < dtSubjectStudent.length; i++ ){%>
        <tr>

          <% if(dtSubjectStudent[i].Student !== null ){ %>
            <td><%= no +=1 %></td>
            <td><%= dtSubjectStudent[i].Student.first_name %> <%= dtSubjectStudent[i].Student.last_name %></td>
          <% } %>

          <% if( dtSubjectStudent[i].score == null && dtSubjectStudent[i].Student !== null){ %>
            <td>
              <a href="/subject/<%= dtSubjectStudent[i].StudentId %>/<%= dtSubjectStudent[i].SubjectId %>/givescore"> Givescore</a>
            </td>
          <% }else if(dtSubjectStudent[i].Student !== null){ %>
            <td><%= dtSubjectStudent[i].score %></td>
          <% } %>
        </tr>
      <% } %>
    </table>
  </body>
</html>
