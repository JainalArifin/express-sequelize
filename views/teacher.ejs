<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <a href="/">Home</a>
    <a href="/teacher">Teacher</a>
    <a href="/subject">Subject</a>
    <a href="/student">Student</a>
    <br><br>
    <h1>Tabel Teacher</h1>
    <table border="1">
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
      <% dtTeacher.forEach(data=>{ %>
        <tr>
          <th><%= data.id %></th>
          <td><%= data.first_name %></td>
          <td><%= data.last_name %></td>
          <td><%= data.email %></td>
        </tr>
      <% }) %>
    </table>
  </body>
</html>
